<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
<mapper namespace="com.config.RoomMapper">
    
   <!--전체 조회 -->
   <select id="All" resultType="RoomDTO">
      select room_no, 
             room_state, 
             room_capacity, 
             room_checkin_time, 
             room_checkout_time
             
      from tb_room
      order by room_no desc
   </select>
</mapper>

